{
  "name": "fatri-vue-ui",
  "version": "1.1.0",
  "description": "A Component Library for Vue.js",
  "main": "lib/fatri-vue-ui.common.js",
  "files": [
    "libs",
    "src",
    "packages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FatriFE/FatriVueUI.git"
  },
  "author": "Star (Shun) Li <Starlee.Li@fatri.cn>",
  "keywords": [
    "vue",
    "fatri",
    "component",
    "fatri-vue-ui"
  ],
  "license": "MIT",
  "scripts": {
    "bootstrap": "yarn || npm i",
    "build:file": "node build/bin/build-entry.js & node build/bin/i18n.js",
    "rm": "rimraf examples/fatri-vue-ui",
    "dev": "npm run build:file && webpack-dev-server --config build/webpack.demo.js & node build/bin/template.js",
    "build:doc": "npm run rm && npm run build:file && cross-env NODE_ENV=production webpack --config build/webpack.demo.js && echo fatri.eleme.io>>examples/fatri-vue-ui/CNAME",
    "deploy:web": "npm run bootstrap&&npm run build:doc && sh build/deploy-web.sh",
    "dist": "webpack --config build/webpack.conf.js && webpack --config build/webpack.common.js && webpack --config build/webpack.component.js",
    "dist:umd": "webpack --config build/webpack.conf.js",
    "dist:common": "webpack --config build/webpack.common.js",
    "dist:com": "webpack --config build/webpack.component.js",
    "release": "standard-version",
    "lint": "eslint src/**/*  packages/**/* build/**/* --fix"
  },
  "bugs": {
    "url": "https://github.com/FatriFE/FatriVueUI/issues"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e $GIT_PARAMS"
  },
  "lint-staged": {
    ".{js|vue}": [
      "yarn lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "chokidar": "^3.3.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^5.1.0",
    "file-save": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "json-templater": "^1.2.0",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "standard-version": "^7.1.0",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "element-ui": "^2.13.0",
    "highlight.js": "^9.18.1",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "throttle-debounce": "^2.1.0",
    "transliteration": "^2.1.8",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
